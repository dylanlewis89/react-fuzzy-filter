{
  "name": "react-fuzzy-filter",
  "version": "0.3.1",
  "description": "Render a subset of a collection with fuzzy searching",
  "main": "dist/react-fuzzy-filter.min.js",
  "scripts": {
    "build": "$(npm bin)/webpack",
    "lint:src": "$(npm bin)/eslint ./src",
    "lint:test": "$(npm bin)/eslint ./test",
    "lint": "npm run lint:src && npm run lint:test",
    "test": "$(npm bin)/mocha -R dot --compilers js:babel-core/register --require ./test/setup.js --recursive",
    "clean": "rm -rf dist",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdlehman/react-fuzzy-filter.git"
  },
  "bugs": {
    "url": "https://github.com/jdlehman/react-fuzzy-filter/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "filter",
    "fuzzy",
    "javascript"
  ],
  "files": [
    "dist"
  ],
  "author": "Jonathan Lehman <jonathan.lehman91@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-config-panoply": "^1.0.2",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.2",
    "mocha": "^2.5.3",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "fuzzysearch": "^1.0.3"
  }
}
