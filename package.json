{
  "name": "react-fuzzy-filter",
  "version": "2.3.0",
  "description": "Render a subset of a collection with fuzzy searching",
  "main": "dist/react-fuzzy-filter.umd.min.js",
  "jsnext:main": "dist/react-fuzzy-filter.es-modules.js",
  "scripts": {
    "build": "npm run clean && $(npm bin)/rollup -c && npm run minify",
    "minify": "npm run minify-browser && npm run minify-umd",
    "minify-browser": "$(npm bin)/uglifyjs dist/react-fuzzy-filter.browser.js -cm > dist/react-fuzzy-filter.browser.min.js",
    "minify-umd": "$(npm bin)/uglifyjs dist/react-fuzzy-filter.umd.js -cm > dist/react-fuzzy-filter.umd.min.js",
    "lint:src": "$(npm bin)/eslint ./src",
    "lint:test": "$(npm bin)/eslint ./test",
    "lint": "npm run lint:src && npm run lint:test",
    "test": "$(npm bin)/mocha",
    "clean": "rm -rf dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdlehman/react-fuzzy-filter.git"
  },
  "bugs": {
    "url": "https://github.com/jdlehman/react-fuzzy-filter/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "filter",
    "fuzzy",
    "javascript"
  ],
  "files": [
    "dist"
  ],
  "author": "Jonathan Lehman <jonathan.lehman91@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-config-panoply": "^1.0.2",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.2",
    "mocha": "^2.5.3",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "debounce": "^1.0.0",
    "fuse.js": "^2.3.0",
    "rxjs": "^5.0.0-beta.10"
  }
}
